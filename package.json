{
  "name": "awesome-products",
  "version": "1.0.0",
  "description": "Store your products and get them direct from the browser!",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack && NODE_ENV=development node_modules/.bin/webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development webpack",
    "lint": "standard --parser babel-eslint --verbose | snazzy",
    "test": "jest --config jest.json",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonardoHabitzreuter/awesome-products.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Leonardo Habitzreuter <leonardo.h@hbsis.com.br> (https://github.com/LeonardoHabitzreuter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeonardoHabitzreuter/awesome-products/issues"
  },
  "homepage": "https://github.com/LeonardoHabitzreuter/awesome-products#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ],
    "globals": [
      "state",
      "it",
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "localStorage",
      "jest",
      "test"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
